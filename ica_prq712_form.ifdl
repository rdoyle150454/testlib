/* This response performs the GET_MPQ_KEY step, passing in parameter info to
   the key panel and receiving the user's parameter selections when 
   P$_REFRESH_FLAG is 0 */

    INTERNAL RESPONSE  DISPLAY_EXT_ID_PANEL
        LET L$_EXT_ID_FOUND = V$_NO

        If (EXT_ID_NUM_TYPE_CODE = V346$VISA_CREDIT_CARD) Then
          CALL 'ICA_PRF_PROD_EXT_ID_SEP_PEU' USING ICA_PRF_PROD_EXT_ID_SEP_PEW
          DISPLAY    VISA_PANEL
          LET L$_EXT_ID_FOUND = V$_YES
        End if

/**ER***2.11.97***b**/
        If (EXT_ID_NUM_TYPE_CODE = V346$DINERS_CARD) Then
          CALL 'ICA_PRF_PROD_EXT_ID_SEP_PEU' USING ICA_PRF_PROD_EXT_ID_SEP_PEW
          DISPLAY    DINERS_PANEL
          LET L$_EXT_ID_FOUND = V$_YES
